{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "target": "ES2017",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "~api/*": ["src/api/*"],
      "~custom-element/*": ["src/custom-element/*"],
      "~event/*": ["src/event/*"],
      "~page/*": ["src/page/*"],
      "~resources/*": ["src/resources/*"],
    }
  },
  "include": [
    "src",
    "vite-env.d.ts"
  ],
  "files": [
    "src/resource.d.ts"
  ]
}